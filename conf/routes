# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()
GET     /login                      controllers.Application.login()

GET		/dashboard					controllers.Application.dashboard()

GET		/admin						controllers.Application.admin()
GET		/clinician					controllers.Application.clinician()
GET		/patient					controllers.Application.patient()

GET		/test						controllers.Application.test(start:String,end:String,title:String,body:String,username:String)
GET		/list						controllers.Application.list()


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
